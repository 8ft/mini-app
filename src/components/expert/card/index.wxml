<navigator class="expert" hover-class="none" url="/pages/expert/index/index?id={{data.userId}}">
    <view class="info">
      <image class="avatar" mode="aspectFill" src="{{data.userAvatar}}"></image>
      <image class="type" mode="aspectFill" src="/assets/img/discover/{{data.settleType===2?'team':'personal'}}.png"></image>
      <view class="last-login">{{data.loginTimeCn}}</view>
      <view class="name"><text>{{data.nickName}}</text>{{data.positionTitle}}</view>
      <view class="label">{{data.cityCn}}</view>
      <view class="label">{{data.workExperience}}年</view>
      <view class="label">完成{{data.finishOrderNums}}单</view>
      <view class="price">{{data.daySalary}}元/日</view>
      <view class="tags">{{data.positionTypeCn}}</view>
    </view>
    <view class="action">
      <view><text class="iconfont icon-xiaoxi1"></text>私信</view>
      <view wx:if="{{remarkable}}"><text wx:if="{{data.talentRemark}}" class="iconfont icon-beizhu"></text>{{data.talentRemark?'已添加备注':'备注'}}</view>
      <view><text class="iconfont icon-{{data.followFlag===0?'shoucang1':'yishoucang'}}"></text>{{data.followFlag===0?'收藏':'已收藏'}}</view>
    </view>
</navigator>
